      <outgoing>Flow_4</outgoing>
    </task>
    <task id="Task_3" name="Mengisi Formulir Pemesanan">
      <incoming>Flow_5</incoming>
      <outgoing>Flow_6</outgoing>
    </task>
    <task id="Task_4" name="Melakukan Pembayaran (Tunai/Transfer)">
      <incoming>Flow_7</incoming>
      <outgoing>Flow_8</outgoing>
    </task>
    <task id="Task_5" name="Mengambil dan Mengembalikan Mobil">
      <incoming>Flow_9</incoming>
